{
  "name": "robin-template",
  "version": "0.0.1",
  "description": "The robin default template",
  "main": "contract/index.ts",
  "typings": "index.d.ts",
  "repository": "https://github.com/ultrain-os/robin-template.git",
  "license": "MIT",
  "keywords": [
    "ultrain robin contract"
  ],
  "scripts": {
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"contract/**/*.ts\" --write",
    "fix:tslint": "tslint --fix --project ."
  },
  "engines": {
    "node": ">=8.9"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "codecov": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "prettier": "^1.10.2",
    "standard-version": "^4.0.0",
    "tslint": "^5.4.3",
    "tslint-config-prettier": "^1.8.0",
    "tslint-immutable": "^4.5.1",
    "typescript": "^2.8.3"
  },
  "ava": {
    "failFast": true,
    "files": [
      "build/**/*.spec.js"
    ],
    "sources": [
      "build/**/*.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": [
      "@types/node"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "ContractSdk": "git+ssh://git@github.com:ultrain-os/TsSDK.git",
    "lint": "git+ssh://git@github.com:ultrain-os/robin-lint.git",
    "u3.js": "https://github.com/ultrain-os/u3.js.git",
    "ultrascript": "git+ssh://git@github.com/ultrain-os/assemblyscript.git"
  }
}
